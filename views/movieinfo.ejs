    <body style="background-color:black;">
        
        <form method="GET" action="/gotomoviesearch">
     <img src= <%= Poster %> width="400" height="450" align="left"style="padding-top:60px;padding-left:30px;" >  
    
          <div style="color:white;padding-left: 550px;padding-top:30px">
            <h1 style="color:rgba(255, 255, 0, 0.986)"><%=Title %></h1>
           <b> Year :</b> <%= Year %> <br><br>
            <b>Ratings :</b> <%= imdbRating %> <br><br>
            <b>  Released :</b> <%= Released %><br><br>
            <b>Genre :</b><%=Genre%><br><br>
            <b>Actors :</b> <%=Actors%> <br><br>
            <b> Plot :</b> <%=Plot %> <br><br>
            <b>Language : </b><%=Language %> <br><br>
            <b>Boxoffice Collection</b> : <%=BoxOffice %> <br><br>
            <button type="submit" class="btn" style="background-color:yellow;" >Search for another movie &nbsp;<span><i class="bi bi-camera-reels-fill"></i></span></button>
           </div>
        </form>
    </body>
</html>

